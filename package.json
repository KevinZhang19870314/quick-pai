    "release-local": "npm run deploy && npm run pack && npm run install-pkg",
    "publish": "cd ./dist/x-controls/ && npm publish",
    "compodoc-lib": "npx compodoc -p projects/x-controls/tsconfig.lib.json"
  },
  "private": true,
